{
    "name": "connect232/carousel-videos",
    "description": "Carousel Videos plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "~4.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "CarouselVideos\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CarouselVideos\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
